<script type="text/javascript">
    var thebox;
    var thr = 123;

    var smMAX_SPEED     = 1000; // max abs speed (px/s)
    var smACC           = 2000; // acceleration (px/s^2)
    var smTIME_ACCEL    = 500; // time of acceleration (ms)
    var smTIME_SLICE    = 25; // ms
    var smDEACCEL_START = smACC*smTIME_ACCEL*smTIME_ACCEL/2; // start deacceleration in px (1/2*a*t^2)
    var smMAX_DIF       = smMAX_SPEED * smTIME_SLICE / 1000;
    var smDIFF2         = smACC * smTIME_SLICE / 1000 * smTIME_SLICE / 1000;

    var smOff = 0;  // target offset
    var smLast = 0; // last offset
    var smDif = 0;  // current speed 
    var smDif2 = 0; // current acceleration
    var smRun = 0;  // flag if running
    var smDir = 0;  // direction

    function setup() {
        thebox = document.getElementById( 'thebox' );
        var thetop = document.getElementById( 'thetop' );
				thr = thetop.offsetHeight + 15;
				var menu = document.getElementById( 'menu' );
				thr += menu.offsetTop + menu.offsetHeight;
				var pc = document.getElementById( 'page_content' );
				if (thebox.offsetHeight + thr + 30 > pc.offsetTop + pc.offsetHeight) return; // not needed
        thebox.style.position = "absolute";
        thebox.style.top = "" + smLast + 'px';
        document.onscroll = adjust;
        adjust();
    }

    function adjust() {
        var woff = window.pageYOffset;
        var off = woff < thr ? 0 : woff - thr;
        smoothly( off );
    }


    function smoothly(off) {
				if (smOff == off) return;
				if (smLast + thebox.offsetHeight < off) smLast = off - thebox.offsetHeight;
				else if (off + window.innerHeight < smLast) smLast = off + window.innerHeight;
        smOff = off;

        if (smRun == 0 ) {
            smRun = 1;
            setTimeout( "smoothTimer()", 500 );
        }
    }

    function smoothTimer() {
        var diff = smOff - smLast;
        if (diff == 0) {
            smDir = 0;
            smDif = 0;
            smDif2 = 0;
            smRun = 0;
            return;
        }

        if (diff < 0 && smDif >= 0 || diff > 0 && smDif <= 0) smDir = 0;
        if (smDir == 0) {
            smDir = diff / Math.abs( diff );
            smDif2 = smDIFF2 * smDir;
        }

        smDif += smDif2;

        if (Math.abs(smDif) > smMAX_DIF) {
            smDif2 = 0;
            smDif = smMAX_DIF * smDir;
        }

        smLast += smDif;

        diff = smOff - smLast;

        if (diff < 0 && smDir > 0 || diff > 0 && smDir < 0) {
            thebox.style.top = "" + smOff + 'px';
            smLast = smOff;
            smDir = 0;
            smDif = 0;
            smDif2 = 0;
            smRun = 0;
        }
        else {
            thebox.style.top = "" + smLast + 'px';
            setTimeout( "smoothTimer()", smTIME_SLICE );
        }
    }

</script>
