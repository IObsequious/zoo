	<grammar>
		<dir>fetched</dir>
		<level>0</level>
		<method>typeover</method>
	</grammar>
	<grammar>
		<dir>extracted</dir>
		<level>1</level>
		<derivedFrom>fetched</derivedFrom>
		<method>automatic</method>
		<!-- TODO: how to refer to shared EDDs? -->
		<fileused>lll2.edd</fileused>
		<toolused>hunter</toolused>
	</grammar>
	<grammar>
		<!-- TODO: connected, useful, etc -->
		<dir>recovered</dir>
		<level>3</level>
		<!-- NB: not from extracted! -->
		<derivedFrom>fetched</derivedFrom>
		<method>semi-automated</method>
		<!-- TODO: how to refer to shared EDDs? -->
		<toolused>gdk</toolused>
        <fileused>deyaccify.trafo</fileused>
        <fileused>adapt.trafo</fileused>
        <fileused>disambiguate.trafo</fileused>
        <fileused>fixerrors.trafo</fileused>
        <fileused>expressions.trafo</fileused>
        <fileused>modularise.trafo</fileused>
	</grammar>
	<grammar>
		<dir>imported</dir>
		<level>3</level>
		<derivedFrom>recovered</derivedFrom>
		<method>automatic</method>
		<!-- TODO: how to refer to shared EDDs? -->
		<fileused>lll2.edd</fileused>
