<?xml version="1.0" encoding="UTF-8"?>
<entry>
	<name>Bezault</name>
	<source>
		<author>Eric Bezault</author>
		<title>Eiffel: The Syntax</title>
		<date>1999</date>
	</source>
	<link>
		<uri>http://www.gobosoft.com/eiffel/syntax/</uri>
		<name>Online</name>
	</link>
	<readme/>
	<grammar>
		<dir>fetched</dir>
		<level>0</level>
		<method>copy</method>
	</grammar>
	<grammar>
		<of>Eiffel</of>
		<dir>extracted</dir>
		<level>1</level>
		<derivedFrom>fetched</derivedFrom>
		<method>recovery</method>
		<fileused>config.edd</fileused>
		<toolused>hunter</toolused>
		<fileused>post-extract.xbgf</fileused>
		<toolused>xbgf</toolused>
	</grammar>
	<grammar>
		<of>Eiffel</of>
		<dir>connected</dir>
		<level>2</level>
		<derivedFrom>extracted</derivedFrom>
		<method>semi-automated</method>
		<fileused>complete.xbgf</fileused>
		<toolused>xbgf</toolused>
	</grammar>
</entry>
