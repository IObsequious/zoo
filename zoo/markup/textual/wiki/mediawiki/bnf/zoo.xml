	<readme/>
	<grammar>
		<dir>fetched</dir>
		<level>0</level>
		<method>download</method>
	</grammar>
	<grammar>
		<of>MediaWiki</of>
		<dir>extracted</dir>
		<level>1</level>
		<method>automatic</method>
		<derivedFrom>fetched</derivedFrom>
		<!-- -->
		<fileused>inline.edd</fileused>
		<fileused>main.edd</fileused>
		<fileused>metawiki.edd</fileused>
		<fileused>noparse.edd</fileused>
		<fileused>plain.edd</fileused>
		<fileused>special.edd</fileused>
		<toolused>hunter</toolused>
	</grammar>
	<grammar>
		<of>MediaWiki</of>
		<dir>connected</dir>
		<!-- TODO: check level! -->
		<level>2</level>
		<method>automatic</method>
		<derivedFrom>extracted</derivedFrom>
		<!-- -->
		<fileused>utilise-repetition.xbgf</fileused>
		<fileused>remove-concatenation.xbgf</fileused>
		<fileused>remove-extension-points.xbgf</fileused>
		<fileused>remove-php-legacy.xbgf</fileused>
		<fileused>deyaccify.xbgf</fileused>
		<fileused>remove-comments.xbgf</fileused>
		<fileused>remove-lookahead.xbgf</fileused>
		<fileused>remove-duplicates.xbgf</fileused>
		<fileused>dehtmlify.xbgf</fileused>
		<fileused>utilise-question.xbgf</fileused>
		<fileused>fix-markup.xbgf</fileused>
		<fileused>define-special-symbols.xbgf</fileused>
		<fileused>fake-exclusion.xbgf</fileused>
		<fileused>remove-postfix-case.xbgf</fileused>
		<fileused>fix-names.xbgf</fileused>
		<fileused>unify-whitespace.xbgf</fileused>
		<fileused>connect-grammar.xbgf</fileused>
		<fileused>refactor-repetition.xbgf</fileused>
		<fileused>define-lexicals.xbgf</fileused>
		<toolused>xbgf</toolused>
	</grammar>
