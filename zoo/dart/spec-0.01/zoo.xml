<?xml version="1.0" encoding="UTF-8"?>
<entry>
	<name>Dart Specification 0.01</name>
	<source>
		<author>The Dart Team</author>
		<title>Dart Programming Language Specification</title>
		<subtitle>Draft Version 0.01</subtitle>
		<date>October 2011</date>
	</source>
	<link>
		<uri>http://www.dartlang.org/docs/spec/index.html</uri>
		<name>Online</name>
	</link>
	<readme/>
	<grammar>
		<dir>fetched</dir>
		<level>0</level>
		<method>copy</method>
	</grammar>
	<grammar>
		<dir>extracted</dir>
		<level>1</level>
		<method>automatic</method>
		<derivedFrom>fetched</derivedFrom>
		<!--  -->
		<toolused>perl</toolused>
		<fileused>ebnf.edd</fileused>
		<toolused>hunter</toolused>
		<fileused>post-extract.xbgf</fileused>
		<toolused>xbgf</toolused>
	</grammar>
	<grammar>
		<dir>corrected</dir>
		<level>2</level>
		<method>recovery</method>
		<derivedFrom>extracted</derivedFrom>
		<!--  -->
		<fileused>fix-spaces.xbgf</fileused>
		<fileused>drop-empty-alternatives.xbgf</fileused>
		<fileused>unite-misspellings.xbgf</fileused>
		<fileused>emulate-negation.xbgf</fileused>
		<fileused>emulate-omission.xbgf</fileused>
		<fileused>correct.xbgf</fileused>
		<fileused>complete.xbgf</fileused>
		<toolused>xbgf</toolused>
	</grammar>
</entry>
