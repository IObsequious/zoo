	</bgf:production>
	<bgf:production>
		<nonterminal>repeated-sequence</nonterminal>
		<bgf:expression>
			<sequence>
				<bgf:expression>
					<terminal>{</terminal>
				</bgf:expression>
				<bgf:expression>
					<nonterminal>definitions-list</nonterminal>
				</bgf:expression>
				<bgf:expression>
