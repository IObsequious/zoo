		<bgf:expression>
			<sequence>
				<bgf:expression>
					<terminal>(*</terminal>
				</bgf:expression>
				<bgf:expression>
					<star>
						<bgf:expression>
							<nonterminal>comment-symbol</nonterminal>
						</bgf:expression>
					</star>
				</bgf:expression>
				<bgf:expression>
					<terminal>*)</terminal>
				</bgf:expression>
			</sequence>
		</bgf:expression>
	</bgf:production>
	<bgf:production>
		<nonterminal>comment-symbol</nonterminal>
		<bgf:expression>
			<choice>
