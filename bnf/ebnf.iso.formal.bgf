					<nonterminal>factor</nonterminal>
				</bgf:expression>
				<bgf:expression>
					<optional>
						<bgf:expression>
							<sequence>
								<bgf:expression>
									<terminal>-</terminal>
								</bgf:expression>
								<bgf:expression>
