				</bgf:expression>
				<bgf:expression>
					<nonterminal>special-sequence</nonterminal>
				</bgf:expression>
				<bgf:expression>
					<nonterminal>character</nonterminal>
				</bgf:expression>
			</choice>
		</bgf:expression>
	</bgf:production>
</bgf:grammar>
