<?xml version="1.0" encoding="UTF-8"?>
<grammar>
	<name>Recovered</name>
	<source>
		<author>The Dart Team</author>
		<title>Dart Programming Language Specification</title>
		<subtitle>Draft Version 0.01</subtitle>
		<date>October 2011</date>
		<readme/>
		<link>
			<uri>http://www.dartlang.org/docs/spec/index.html</uri>
			<name>Online</name>
		</link>
	</source>
	<extraction>
		<edd>ebnf</edd>
		<make>replacements</make>
		<xbgf>post-extract</xbgf>
	</extraction>
	<recovery>
		<xbgf>fix-spaces</xbgf>
		<xbgf>drop-empty-alternatives</xbgf>
		<xbgf>unite-misspellings</xbgf>
		<xbgf>emulate-negation</xbgf>
		<xbgf>emulate-omission</xbgf>
		<xbgf>correct</xbgf>
		<xbgf>complete</xbgf>
	</recovery>
</grammar>
